<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="SunnyUI" Version="3.8.7" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\MTs\MTs.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Views\FlowControl.cs">
      <SubType>UserControl</SubType>
    </Compile>
    <Compile Update="Views\FlowControl.Designer.cs">
      <DependentUpon>FlowControl.cs</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Views\FlowControl.resx">
      <DependentUpon>FlowControl.cs</DependentUpon>
    </EmbeddedResource>
  </ItemGroup>

</Project>